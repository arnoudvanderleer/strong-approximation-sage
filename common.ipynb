   "source": [
    "def ord(p, x):\n",
    "    if x == 0:\n",
    "        return Infinity\n",
    "    return ([n for (q, n) in x.factor() if q == p] + [0])[0]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def ord_vector(p, m, column):\n",
    "    return min(ord(p, x) for x in m[:,column].coefficients())"
   ]
  }
